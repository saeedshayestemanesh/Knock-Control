	state {
		id                  		3
		labelString         		"eML_blk_kernel()"
		position            		[18 64.5 118 66]
		fontSize            		12
		chart               		2
		treeNode            		[2 0 0 0]
		superState          		SUBCHART
		subviewer           		2
		type                		FUNC_STATE
		decomposition       		CLUSTER_STATE
		eml {
			isEML       		1
			script      		"function relSpark = knCtrl(knocking,initialSpark,retardGain"
						",advanceGain)\n"
						"\n"
						"%Computes the new relative spark angle (deg) based on:\n"
						"%   knocking:       a boolean knock signal\n"
						"%   initialSpark:   the initial spark relative to borderlin"
						"e\n"
						"%   retardGain:     the retard amount (deg)\n"
						"%   advanceGain     the advance amount (deg)\n"
						"\n"
						"%   Copyright 2009 Villanova University\n"
						"%   Author: James C. Peyton Jones  (5/1/2009)\n"
						"%   Version knock0:  Traditional Knock Control\n"
						"%   Revision:        Date:\n"
						"%\n"
						"%\n"
						"\n"
						"%Declare persistent variables\n"
						"persistent relSpk\n"
						"\n"
						"%Initialization\n"
						"if isempty(relSpk),\n"
						"    relSpk= double(initialSpark);\n"
						"end;\n"
						"\n"
						"%Traditional knock control\n"
						"if knocking,\n"
						"\trelSpk= relSpk - retardGain;\n"
						"else\n"
						"\trelSpk= relSpk + advanceGain;\n"
						"end;\n"
						"\n"
						"%Apply saturation limits\n"
						"if relSpk>4, relSpk=4; end;\n"
						"if relSpk<-4, relSpk=-4; end;\n"
						"\n"
						"%Output value\n"
						"relSpark= double(relSpk);\n"
						""
			editorLayout		"100 M4x1[45 45 710 799]"
		}
	}

	junction {
		id         		4
		position   		[23.5747 49.5747 7]
